@{
    string cssClass = ViewData["CssClass"] as string;
    string equation = ViewData["Equation"] as string;
    string message = ViewData["Message"] as string;
}

<!DOCTYPE html>
<html lang="pl">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Rozwiązanie równania kwadratowego</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <div class="@cssClass">
        <p>@equation</p>
        <p>@message</p>
    </div>
</body>
</html>


